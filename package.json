{
	"name": "sveltekit-office-addin",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"start": "node build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"office": "office-addin-debugging start ./office_manifests/excel_manifest.xml",
		"stop": "office-addin-debugging stop ./office_manifests/excel_manifest.xml",
		"validate": "office-toolbox validate -m ./office_manifests/excel_manifest.xml"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@types/office-js": "^1.0.390",
		"@vitejs/plugin-basic-ssl": "^1.2.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-office-addins": "^3.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"office-addin-debugging": "^5.1.2",
		"office-addin-lint": "^2.3.2",
		"office-addin-prettier-config": "^1.2.0",
		"office-toolbox": "^0.3.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@microsoft/office-js": "^1.1.101",
		"vite-plugin-html": "^3.2.2",
		"vite-plugin-mkcert": "^1.17.6"
	}
}